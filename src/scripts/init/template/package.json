{
  "license": "ISC",
  "version": "1.0.0",
  "name": "my-package",
  "homepage": "https://github.com/arcaelas/cli#readme",
  "description": "The library is easy to use, and it can help developers to save time and effort when creating new modules.",
  "main": "./lib/index.js",
  "exports": {
    "default": "./lib/index.js",
    "require": "./lib/index.cjs",
    "import": "./lib/index.js"
  },
  "files": [
    "lib/",
    "*.md",
    "*.json"
  ],
  "author": {
    "name": "Arcaelas Insiders",
    "email": "comunity@arcaelas.com",
    "url": "https://github.com/arcaelas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arcaelas/cli.git"
  },
  "bugs": {
    "email": "community@arcaelas.com",
    "url": "https://github.com/arcaelas/.github/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && ts-node esbuild.js",
    "commit": "npm publish --access=public",
    "prepublishOnly": "npm run build && npm version minor",
    "postpublish": "rm -rf lib"
  },
  "keywords": [
    "command",
    "cmd",
    "arcaelas",
    "@arcaelas",
    "framework",
    "terminal",
    "console",
    "logger"
  ],
  "dependencies": {
    "@arcaelas/utils": "^1.24.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "esbuild": "^0.17.18",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}