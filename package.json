  "name": "@arcaelas/cli",
  "main": "lib/index.js",
  "bin": {
    "arcaelas": "lib/index.js"
  },
  "description": "This library is used to create modules from scratch with linter and configs files for default.",
  "scripts": {
    "test": "jest",
    "start": "tsc --watch",
    "commit": "npm publish --access=public",
    "prepublishOnly": "tsc && npm run build:create && npm version minor",
    "build:create": "shx rm -rf lib/scripts/create/templates && shx cp -r src/scripts/create/templates lib/scripts/create/templates",
    "postpublish": "shx rm -rf lib"
  },
  "keywords": [
    "arcaelas",
    "module creation",
    "linting",
    "config files",
    "software development",
    "node.js",
